{
  "name": "@heripo/model",
  "private": false,
  "type": "module",
  "version": "0.1.0",
  "description": "Document models and type definitions for heripo engine",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "NOTICE"
  ],
  "sideEffects": false,
  "author": "heripo lab",
  "contributors": [
    "Kim Hongyeon <kimhongyeon89@gmail.com>",
    "Cho Hayoung <hmys0500@gmail.com>"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/heripo-lab/heripo-engine",
    "directory": "packages/model"
  },
  "bugs": {
    "url": "https://github.com/heripo-lab/heripo-engine/issues"
  },
  "homepage": "https://github.com/heripo-lab/heripo-engine/tree/main/packages/model",
  "keywords": [
    "heripo",
    "document",
    "model",
    "typescript",
    "archaeology"
  ],
  "engines": {
    "node": ">=22"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm clean && tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepublishOnly": "pnpm build",
    "preversion": "pnpm build",
    "postversion": "git push --follow-tags",
    "release": "pnpm pack && npm publish *.tgz --access public && rm *.tgz",
    "release:patch": "pnpm version patch && pnpm pack && npm publish *.tgz --access public && rm *.tgz",
    "release:minor": "pnpm version minor && pnpm pack && npm publish *.tgz --access public && rm *.tgz",
    "release:major": "pnpm version major && pnpm pack && npm publish *.tgz --access public && rm *.tgz"
  },
  "devDependencies": {
    "@heripo/tsconfig": "workspace:*",
    "@heripo/tsup-config": "workspace:*",
    "tsup": "catalog:"
  }
}
